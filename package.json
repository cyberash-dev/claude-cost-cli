{
  "name": "claude-usage-cli",
  "version": "0.1.0",
  "type": "module",
  "description": "CLI for Claude API usage and cost reports",
  "main": "dist/index.mjs",
  "bin": {
    "claude-usage": "./dist/index.mjs"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.mjs",
    "lint": "biome lint",
    "format": "biome format --write",
    "check": "biome check --write"
  },
  "keywords": [
    "claude",
    "anthropic",
    "usage",
    "cost",
    "cli",
    "api"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyberash-dev/claude-usage-cli.git"
  },
  "bugs": {
    "url": "https://github.com/cyberash-dev/claude-usage-cli/issues"
  },
  "homepage": "https://github.com/cyberash-dev/claude-usage-cli#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@inquirer/prompts": "8.2.0",
    "chalk": "5.6.2",
    "cli-table3": "0.6.5",
    "commander": "14.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/node": "25.2.3",
    "tsdown": "0.20.3",
    "typescript": "5.9.3"
  }
}
